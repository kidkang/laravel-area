    "name": "yjtec/laravel-area",

    "license": "MIT",
    "authors": [
        {
            "name": "kidkang",
            "email": "723616880@qq.com"
        }
    ],
    "require": {
        "php" : "^7.2.5|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yjtec\\Area\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yjtec\\Area\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yjtec\\Area\\AreaServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
